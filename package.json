{
  "name": "machine-mall-monorepo",
  "private": true,
  "workspaces": [
    "apps/*"
  ],
  "scripts": {
    "dev:client": "npm run dev --workspace=clonemachemall",
    "build:client": "npm run build --workspace=clonemachemall",
    "start:client": "npm run start --workspace=clonemachemall",
    "dev:admin": "npm run dev --workspace=admin",
    "build:admin": "npm run build --workspace=admin",
    "dev:server": "npm run start:dev --workspace=server",
    "build:server": "npm run build --workspace=server"
  }
}
